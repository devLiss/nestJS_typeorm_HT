{
  "version": 2,
  "name": "nest",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "crons": [
    {
      "path": "/pair-game-quiz/cron",
      "schedule": "0/3 * * * *"
    }
  ]
}
